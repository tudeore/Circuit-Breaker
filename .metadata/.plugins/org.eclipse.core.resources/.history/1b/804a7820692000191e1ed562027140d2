		RestTemplate restTemplate = new RestTemplate();
		URI uri = URI.create("http://localhost:8181/hello");
		
		return null;
		
	}
	

	public static void main(String[] args) {
		SpringApplication.run(HelloWebApplication.class, args);
	}

}

